            selection.InsertAfter (currencies(counter, 1) + " ")
        End If
        found = True
        Exit For
    End If
Next counter

If found = False Then
    If selection.Range.ComputeStatistics(wdStatisticWords) = 1 And iterations < 1 Then
        country = UCase("*" + InputBox("Please input country name:") + "*")
        iterations = 1
        GoTo Check
    End If
    MsgBox "Country not found."
End If

End Sub
